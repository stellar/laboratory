{
  "name": "stellar-laboratory",
  "version": "0.1.0",
  "author": "Stellar Development Foundation <stellar@stellar.org>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "NEXT_PUBLIC_COMMIT_HASH=$(git rev-parse --short HEAD) NEXT_PUBLIC_ENABLE_EXPLORER=true next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:ts": "tsc --noEmit",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test": "yarn test:unit && yarn test:e2e",
    "install-if-package-changed": "git diff-tree -r --name-only --no-commit-id ORIG_HEAD HEAD | grep --quiet yarn.lock && yarn install || exit 0",
    "prepare": "husky",
    "pre-commit": "yarn lint-staged",
    "pre-push": "yarn lint:ts && yarn test:e2e && yarn test:unit",
    "playwright-ui": "npx playwright test --ui"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.20.1",
    "@creit.tech/stellar-wallets-kit": "^1.7.7",
    "@ledgerhq/hw-app-str": "^7.2.4",
    "@ledgerhq/hw-transport-webusb": "^6.29.8",
    "@monaco-editor/react": "^4.7.0",
    "@next/third-parties": "^15.4.4",
    "@sentry/nextjs": "^10",
    "@stellar/design-system": "^3.1.5",
    "@stellar/stellar-sdk": "^14.1.1",
    "@stellar/stellar-xdr-json": "^23.0.0-rc.1",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@trezor/connect-plugin-stellar": "^9.0.6",
    "@trezor/connect-web": "^9.6.2",
    "bignumber.js": "^9.3.1",
    "dompurify": "^3.2.5",
    "html-react-parser": "^5.2.6",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "lossless-json": "^4.0.2",
    "next": "15.4.4",
    "papaparse": "^5.5.2",
    "react": "^19.1.1",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^19.1.1",
    "stellar-hd-wallet": "^1.0.2",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.6",
    "zustand-querystring": "^0.0.19"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.4.4",
    "@playwright/test": "^1.54.1",
    "@stellar/tsconfig": "^1.0.2",
    "@types/jest": "^30.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.1.0",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "sass": "^1.86.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
